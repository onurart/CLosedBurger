@model IList<Category>
<div class="col-xl-6 col-xxl-8 col-lg-8">
    <div class="card">
        <div class="card-header bg-info">

            <a href="javascript:void(0);" class="btn rounded text-white border border-white">
                <i class="fa fa-list-alt" aria-hidden="true"></i>
                Top 10 Category List
            </a>
        </div>
        <div class="card-body p-0">
            <div class="table-responsive">
                <table class="table shadow-hover">
                    <tbody>
                        @foreach (var item in Model)
                        {

                            <tr>
                                <td>
                                    <p class="text-black mb-1 font-w600">@item.Name</p>
                                    <span class="fs-14"></span>
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>